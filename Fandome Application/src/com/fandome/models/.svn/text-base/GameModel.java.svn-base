package com.fandome.models;

import java.io.Serializable;
import java.util.Date;

public class GameModel implements Serializable{
	/**
	 * 
	 */
	private static final long serialVersionUID = -2263661280338281395L;
	private String objectId;
	private TeamModel home;
	private TeamModel away;
	private Date gameDate;
	private GameDetailsModel homeGameDetails;
	private GameDetailsModel awayGameDetails;

	public String getObjectId() {
		return objectId;
	}

	public void setObjectId(String objectId) {
		this.objectId = objectId;
	}

	public TeamModel getHome() {
		return home;
	}

	public void setHome(TeamModel home) {
		this.home = home;
	}

	public TeamModel getAway() {
		return away;
	}

	public void setAway(TeamModel away) {
		this.away = away;
	}

	public Date getGameDate() {
		return gameDate;
	}

	public void setGameDate(Date gameDate) {
		this.gameDate = gameDate;
	}

	public GameDetailsModel getHomeGameDetails() {
		return homeGameDetails;
	}

	public void setHomeGameDetails(GameDetailsModel homeGameDetails) {
		this.homeGameDetails = homeGameDetails;
	}

	public GameDetailsModel getAwayGameDetails() {
		return awayGameDetails;
	}

	public void setAwayGameDetails(GameDetailsModel awayGameDetails) {
		this.awayGameDetails = awayGameDetails;
	}
}
